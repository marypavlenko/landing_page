.paralax-budda {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.budda {
  position: absolute;
  z-index: -1;
  left: 0;
  right: 0;
  height: 35%;
  background-image: url("../images/content/budda.png");
  background-size: cover;
  background-repeat: no-repeat;
  background-position: 50% 0;
}